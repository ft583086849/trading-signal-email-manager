{
  "version": 2,
  "builds": [
    {
      "src": "vercel_email_manager.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/subscribers.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/subscribers/[email].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/vercel_email_manager.html"
    },
    {
      "src": "/email-manager",
      "dest": "/vercel_email_manager.html"
    },
    {
      "src": "/api/subscribers",
      "dest": "/api/subscribers.js"
    },
    {
      "src": "/api/subscribers/(.*)",
      "dest": "/api/subscribers/[email].js"
    }
  ]
}